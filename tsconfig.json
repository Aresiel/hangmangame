{
  "compilerOptions": {
    "target": "ES2021",
    "noErrorTruncation": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "types": [
      "bun-types"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    // "outDir": "dist",
    "noEmit": true
  },
  "include": ["src"]
}